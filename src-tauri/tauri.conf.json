{
	"build": {
		"devPath": "../src",
		"distDir": "../src",
		"withGlobalTauri": true
	},
	"package": {
		"productName": "webrtc",
		"version": "0.0.0"
	},
	"tauri": {
		"allowlist": {
			"all": true,
			"window": {
				"all": true
			},
			"fs": {
				"all": true
			}
		},
		"windows": [
			{
				"title": "My App",
				"width": 800,
				"height": 600,
				"resizable": true,
				"fullscreen": true,
				"decorations": true
			}
		],
		"security": {
			"csp": null
		},
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "com.tauri.dev",
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			]
		}
	}
}
